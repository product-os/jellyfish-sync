{
  "name": "@balena/jellyfish-sync",
  "version": "6.1.110",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-sync.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Sync library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest lib",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.97",
    "@balena/jellyfish-logger": "3.0.152",
    "@balena/jellyfish-metrics": "^1.0.417",
    "bluebird": "^3.7.2",
    "json-e": "^4.4.3",
    "json-schema": "^0.4.0",
    "lodash": "^4.17.21",
    "randomstring": "^1.2.1",
    "request": "^2.88.2",
    "typed-error": "^3.2.1"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^1.5.17",
    "@balena/jellyfish-types": "^1.2.34",
    "@balena/lint": "^6.2.0",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.0.3",
    "@types/json-schema": "^7.0.9",
    "@types/request": "^2.48.7",
    "@types/sinon": "^10.0.6",
    "@types/uuid": "^8.3.3",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "dotenv": "^10.0.0",
    "fast-json-patch": "^3.1.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "nock": "^13.2.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "sinon": "^12.0.1",
    "skhema": "^5.3.4",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
