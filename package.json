{
  "name": "@balena/jellyfish-sync",
  "version": "4.1.28",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-sync.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Sync library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js > README.md",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.53",
    "@balena/jellyfish-logger": "0.0.231",
    "@balena/jellyfish-metrics": "^0.1.55",
    "bluebird": "^3.7.2",
    "fast-json-patch": "^3.0.0-1",
    "json-e": "^4.3.0",
    "lodash": "^4.17.20",
    "marked": "^1.2.8",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.3.1",
    "deplint": "^1.1.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^5.0.8",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.3",
    "skhema": "^5.3.3",
    "nock": "^13.0.7"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  }
}
