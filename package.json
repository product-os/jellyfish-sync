{
  "name": "@balena/jellyfish-sync",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-sync.git"
  },
  "description": "Sync library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "lint": "balena-lint --typescript lib test",
    "lint-fix": "balena-lint --typescript --fix lib test",
    "unit": "jest",
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "doc": "NODE_OPTIONS=--max_old_space_size=4096 typedoc --gitRevision master --out docs lib/",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.11",
    "@balena/jellyfish-environment": "^2.2.23",
    "@balena/jellyfish-metrics": "^0.0.71",
    "@balena/jellyfish-uuid": "^1.0.12",
    "@octokit/auth-app": "^2.4.15",
    "@octokit/plugin-retry": "^3.0.3",
    "@octokit/plugin-throttling": "^3.3.0",
    "@octokit/rest": "^18.0.5",
    "@types/bluebird": "^3.5.32",
    "@types/geoip-lite": "^1.1.31",
    "@types/intercom-client": "^2.11.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lru-cache": "^5.1.0",
    "@types/marked": "^1.1.0",
    "@types/node-jose": "^1.1.4",
    "@types/randomstring": "^1.1.6",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "balena-sdk": "^15.10.4",
    "bluebird": "^3.7.2",
    "front-sdk": "^0.8.1",
    "geoip-lite": "^1.4.2",
    "intercom-client": "^2.11.0",
    "is-uuid": "^1.0.2",
    "json-e": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "lru-cache": "^6.0.0",
    "marked": "^1.1.1",
    "native-url": "^0.3.4",
    "node-jose": "^1.1.4",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.0.7",
    "@balena/lint": "^5.1.0",
    "@types/jest": "^26.0.7",
    "@types/nock": "^11.1.0",
    "ava": "^3.12.1",
    "ava-ts": "^0.25.2",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.2.0",
    "deplint": "^1.1.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.4.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^26.2.1",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.3.0",
    "nock": "^13.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typedoc": "^0.19.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "npm run doc"
    ]
  }
