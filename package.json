{
  "name": "@balena/jellyfish-sync",
  "version": "6.0.235",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-sync.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Sync library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=shellcheck,@types/jest",
    "lint-fix": "balena-lint --fix lib",
    "unit": "jest lib",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.46",
    "@balena/jellyfish-logger": "3.0.14",
    "@balena/jellyfish-metrics": "^1.0.269",
    "bluebird": "^3.7.2",
    "json-e": "^4.4.1",
    "json-schema": "^0.3.0",
    "lodash": "^4.17.21",
    "randomstring": "^1.2.1",
    "request": "^2.88.2",
    "typed-error": "^3.2.1"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-types": "^0.6.8",
    "@balena/lint": "^6.1.1",
    "@types/bluebird": "^3.5.35",
    "@types/jest": "^26.0.23",
    "@types/json-schema": "^7.0.7",
    "@types/request": "^2.48.5",
    "@types/sinon": "^10.0.2",
    "@types/uuid": "^8.3.0",
    "depcheck": "^1.4.1",
    "deplint": "^1.1.3",
    "dotenv": "^10.0.0",
    "fast-json-patch": "^3.0.0-1",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "nock": "^13.1.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.4.1",
    "sinon": "^11.1.1",
    "skhema": "^5.3.4",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.2",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
