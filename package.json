{
  "name": "@balena/jellyfish-sync",
  "version": "6.0.16",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-sync.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Sync library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint-fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint --typescript lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=shellcheck,@types/jest",
    "lint-fix": "balena-lint --typescript --fix lib",
    "unit": "jest lib",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.7",
    "@balena/jellyfish-logger": "2.1.27",
    "@balena/jellyfish-metrics": "^1.0.104",
    "bluebird": "^3.7.2",
    "json-e": "^4.4.1",
    "json-schema": "^0.3.0",
    "lodash": "^4.17.21",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "typed-error": "^3.2.1"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-types": "^0.5.13",
    "@balena/lint": "^5.4.1",
    "@types/bluebird": "^3.5.33",
    "@types/jest": "^26.0.22",
    "@types/json-schema": "^7.0.7",
    "@types/request": "^2.48.5",
    "@types/sinon": "^9.0.11",
    "@types/uuid": "^8.3.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "dotenv": "^8.2.0",
    "fast-json-patch": "^3.0.0-1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.11",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.3.1",
    "sinon": "^10.0.1",
    "skhema": "^5.3.3",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
