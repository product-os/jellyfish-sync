{
  "name": "@balena/jellyfish-sync",
  "version": "5.2.86",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-sync.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Sync library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.1.6",
    "@balena/jellyfish-logger": "2.1.24",
    "@balena/jellyfish-metrics": "^1.0.75",
    "bluebird": "^3.7.2",
    "json-e": "^4.4.1",
    "lodash": "^4.17.21",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "fast-json-patch": "^3.0.0-1",
    "jsdoc-to-markdown": "^7.0.1",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.11",
    "simple-git-hooks": "^2.3.1",
    "sinon": "^10.0.1",
    "skhema": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
