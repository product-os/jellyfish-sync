    "@octokit/auth-app": "^2.10.5",
    "@octokit/plugin-retry": "^3.0.6",
    "@octokit/plugin-throttling": "^3.3.4",
    "@octokit/rest": "^18.0.12",
    "bluebird": "^3.7.2",
    "front-sdk": "^0.8.1",
