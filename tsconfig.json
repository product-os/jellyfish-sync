  "compilerOptions": {
    "module": "commonjs",
    "outDir": "build",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": false,
    "target": "es2015",
    "declaration": true,
    "skipLibCheck": true
  },
  "include": ["lib/**/*.ts", "test/index.spec.ts"],
  "exclude": ["lib/integrations/front/index.old.ts"]
}
